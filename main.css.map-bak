-height: 1;\r\n  font-size: 2.5rem;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n  font-family: $fontAnton;\r\n}\r\n.button:hover{\r\n  background-color: $secondaryColor;\r\n  color: white;\r\n}\r\n\r\n.button.invert{\r\n  background-color: black;\r\n  color: white;\r\n  border: 0px;\r\n  padding: 20px 50px;\r\n  font-weight: bold;\r\n  line-height: 1;\r\n  font-size: 2.5rem;\r\n  cursor: pointer;\r\n  font-family: $fontAnton;\r\n}\r\n.button.invert:hover{\r\n  background-color: white;\r\n  color: $secondaryColor;\r\n}\r\n\r\ntable {\r\n    border-collapse: collapse;\r\n    border: 1px solid #666;\r\n    width: 100%;\r\n    text-align: center;\r\n}\r\ntbody tr:nth-child(odd) {\r\n    background-color: #ccc;\r\n}\r\n\r\n\r\n\r\n.blogposts{\r\n  margin-bottom: 50px;\r\n}","/** COLORS */\r\n$textColor     : #000000;\r\n$primaryColor  : #aa1211; // Dark Blue\r\n$secondaryColor: #414042; // Dark Yellow\r\n$darkGray      : #b8b8b8;\r\n$mediumGray    : #b8b8b8; // footer contact bg\r\n$lightGray     : #ebebeb;\r\n$linkColor     : $primaryColor;\r\n\r\n\r\n\r\n$fontLato: 'Montserrat', sans-serif;\r\n$fontAnton: 'Montserrat', sans-serif;\r\n$fontOswald: 'Montserrat', sans-serif;\r\n\r\n$screen-lg-min: 1199px;\r\n$screen-lg-max: ($screen-lg-min - 1);\r\n$screen-md-min: 992px;\r\n$screen-md-max: ($screen-md-min - 1);\r\n$screen-sm-min: 768px;\r\n$screen-sm-max: ($screen-sm-min - 1);\r\n$screen-xsm-min: 576px;\r\n$screen-xsm-max: ($screen-xsm-min - 1);\r\n\r\n/** FONT SIZES */\r\n\r\n$htmlfs          : 10px; //10px\r\n$fsBody          : 2rem; // 20px\r\n$fsTitle         : 5rem; // 50px\r\n$fsH1            : 4rem; // 30px\r\n$fsH2            : 3rem; // 27px\r\n$fsH3            : 2.5rem; // 22px\r\n$fsH4            : $fsBody; // 20px\r\n\r\n/** PARTIALS */\r\n@import \"base\";\r\n@import \"header\";\r\n@import \"forms\";\r\n@import \"woocommerce\";\r\n@import \"navigation\";\r\n@import \"mobile_nav\";\r\n@import \"content\";\r\n@import \"blog\";\r\n@import \"footer\";\r\n@import \"sliders\";\r\n@import \"slides\";\r\n@import \"overrides\";\r\n@import \"ie\";\r\n@import \"responsive\";\r\n","body.sticky-header{\r\n  margin-top: 150px;\r\n}\r\n.site-header {\r\n    box-shadow: 0 3px 20px rgba(black,0.2);\r\n    height: 150px;\r\n    background-color: $secondaryColor;\r\n}\r\n.home .site-header {\r\n    overflow: hidden;\r\n    position: relative;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    min-height: 100vh;\r\n    padding: 0px;\r\n    .flex-header{\r\n        height: 150px;\r\n        z-index: 100;\r\n        position: relative;\r\n        top: 0px;\r\n    }\r\n}\r\n#videoplaceholder{\r\n  opacity: 0.5;\r\n  line-height: 0;\r\n  position: absolute;\r\n  top: 0;\r\n  width: 100%;\r\n  iframe{\r\n    width: 100%;\r\n    min-height: 110vh;\r\n    height: inherit;\r\n  }\r\n}\r\n.flex-header {\r\n    height: 100%;\r\n    display: flex;\r\n    flex-flow: row wrap;\r\n    justify-content: space-between;\r\n    align-items: flex-end;\r\n    margin: 0 auto;\r\n    .logo {\r\n        flex: 0;\r\n        flex: 0 0 17%;\r\n        align-self: center;\r\n        img {\r\n          max-width: 100px;\r\n        }\r\n    }\r\n    .stickylogo {\r\n        opacity: 0;\r\n        transition: opacity 0.2s ease-in-out;\r\n    }\r\n    .header-nav{\r\n      flex: 0 0 83%;\r\n      .container{\r\n        width: 100%;\r\n      }\r\n    }\r\n\r\n    .mobile-icons{\r\n      display: none;\r\n    }\r\n}\r\n.stickylogo img {\r\n    width: 0;\r\n    height: 0;\r\n    overflow: hidden;\r\n    -webkit-transition: all 0.2s ease-in-out;\r\n    transition: all 0.2s ease-in-out;\r\n}\r\n.stickycontainer{\r\n  display: flex;\r\n  flex: 0 0 100%;\r\n  height: 150px;\r\n}\r\n.sticky-header {\r\n    .stickycontainer{\r\n      max-width: 1140px;\r\n      margin: auto;\r\n      width: 100%;\r\n    }\r\n    .flex-header {\r\n        position: fixed!important;\r\n        z-index: 99989;\r\n        left: 0;\r\n        top: 0;